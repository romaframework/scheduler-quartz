<?xml version="1.0" encoding="UTF-8"?>
<class xmlns="http://www.romaframework.org/xml/roma" xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
	xsd:schemaLocation="http://www.romaframework.org/xml/roma http://www.romaframework.org/schema/v3/roma.xsd">
	<aspects>
		<view>
			<form>
				<area name="main" type="column">
					<area name="row" type="row">
						<area name="prevYear" />
						<area name="year" type="row" />
						<area name="nextYear" />
						<area name="prevMonth" />
						<area name="month" type="row" />
						<area name="nextMonth" />
						<area name="refreshCalendar" />
					</area>
					<area name="calendar" type="column" />
					<area name="actions" type="row" />
					<area name="fields" type="form" size="1" />
				</area>
			</form>
		</view>
	</aspects>
</class>
